#include <stdio.h>
int eo (int a)
{
    if (a%2==0)
        return 1;
    else
        return 0;
}
int main()
{
    int a;
    printf ("Enter an integer number: ");
    scanf ("%d",&a);
    if (eo(a)==1)
        printf("The given number is Even.\n");
    else
        printf("The given number is Odd.\n");
    return 0;
}
