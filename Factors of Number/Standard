#include <stdio.h>
int main()
{
    int num,c=0;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    printf ("All Factors of %d: ",num);
    for (int i=1; i<=num; i++)
    {
        if (num%i==0)
        {
            printf("%d ",i);
            c++;
        }
    }
    printf("\nTotal factors: %d\n",c);
    return 0;
}
