#include <stdio.h>
int main()
{
    int a,b,c;
    printf ("Enter three integers: ");
    scanf ("%d %d %d",&a,&b,&c);
    switch (a>b)
    {
    case 1:
        switch (a>c)
        {
        case 1:
            printf ("%d is Greatest.",a);
            break;
        default:
            printf ("%d is Greatest.",c);
        }
        break;
    case 0:
        switch (b>c)
        {
        case 1:
            printf ("%d is Greatest.",b);
            break;
        default:
            printf ("%d is Greatest.",c);
        }
        break;
    }
    return 0;
}
