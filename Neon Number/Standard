#include <stdio.h>
int main()
{
    int num;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    int sq=num*num;
    int sum=0;
    while (sq>0)
    {
        int d=sq%10;
        sum+=d;
        sq/=10;
    }
    if (sum==num)
        printf("The number %d is neon\n",num);
    else
        printf("The number %d is not neon\n",num);
    return 0;
}
