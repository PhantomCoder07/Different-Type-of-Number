#include <stdio.h>
int main()
{
    int n,m,c=0;
    printf ("Enter the inrervals: ");
    scanf ("%d %d",&n,&m);
    printf("Prime numbers between %d to %d are: ",n,m);
    for (int i=n; i<=m; i++)
    {
        int count=0;
        for (int j=1; j<=i; j++)
        {
            if (i%j==0)
            {
                count++;
            }
        }
        if (count==2 || i==1)
        {
            printf("%d ",i);
            c++;
        }
    }
    printf("\nTotal prime numbers: %d\n",c);
    return 0;
}
