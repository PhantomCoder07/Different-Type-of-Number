#include <stdio.h>
int main()
{
    int i,num,flag=0;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    if (num==1 || num==0)
        flag=1;
    for (i=2; i<=num/2; i++)
    {
        if (num%i==0)
        {
            flag=1;
            break;
        }
    }
    if (flag==0)
        printf ("The number %d is Prime\n",num);
    else
        printf ("The number %d is not Prime\n",num);
    return 0;
}
