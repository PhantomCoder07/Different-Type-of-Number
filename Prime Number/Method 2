#include <stdio.h>
int main()
{
    int i=1,num,count=0;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    while (i<=num)
    {
        if (num%i==0)
        {
            count++;
        }
        i++;
    }
    if (count==2)
        printf ("The number %d is Prime\n",num);
    else
        printf ("The number %d is not Prime\n",num);
    return 0;
}
