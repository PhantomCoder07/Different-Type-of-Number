#include <stdio.h>
int main()
{
    int i,num,count=0;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    for (i=1; i<=num; i++)
    {
        if (num%i==0)
        {
            count++;
        }
    }
    if (count==2)
        printf ("The number %d is Prime\n",num);
    else
        printf ("The number %d is not Prime\n",num);
    return 0;
}
