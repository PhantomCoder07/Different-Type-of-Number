#include <stdio.h>
int main()
{
    int num;
    printf ("Enter the number: ");
    scanf ("%d",&num);
    printf("Prime numbers up to %d are: ",num);
    for (int i=1; i<=num; i++)
    {
        int count=0;
        for (int j=1; j<=i; j++)
        {
            if (i%j==0)
            {
                count++;
            }
        }
        if (count==2 || i==1)
            printf("%d ",i);
    }
    return 0;
}
