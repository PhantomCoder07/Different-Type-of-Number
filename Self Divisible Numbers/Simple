#include <stdio.h>
int main()
{
    int n;
    printf("Enter the number: ");
    scanf("%d",&n);
    int m=n,f=0;
    while (m>0)
    {
        int d=m%10;
        if (n%d!=0)
            f=1;
        m/=10;
    }
    if (f==0)
        printf("The given number %d is Self-divisible\n",n);
    else
        printf("The given number %d is not Self-divisible\n",n);
    return 0;
}
