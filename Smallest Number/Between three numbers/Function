#include <stdio.h>
int large (int a, int b, int c)
{
    if (a<=b && a<=c)
        return a;
    else if (b<=a && b<=c)
        return b;
    else
        return c;
}
int main()
{
    int a,b,c;
    printf ("Enter three different numbers: ");
    scanf ("%d %d %d",&a,&b,&c);
    printf("%d is smallest\n",large(a,b,c));
    return 0;
}
