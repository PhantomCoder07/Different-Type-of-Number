#include <stdio.h>
int main()
{
    int a,b;
    printf ("Enter two numbers: ");
    scanf ("%d %d",&a,&b);
    switch (a<b)
    {
    case 1:
        printf ("%d is Smallest.\n",a);
        break;
    case 0:
        switch (b<a)
        {
        case 1:
            printf ("%d is Smallest.\n",b);
            break;
        default:
            printf ("Both are equal\n");
        }
    }
    return 0;
}
